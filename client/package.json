    "prop-types": "^15.5.9",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-motion": "^0.4.8",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.7.2",
