    "cors": "^2.8.3",
    "cron": "^1.2.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "global": "^4.3.1",
    "history": "^4.6.1",
